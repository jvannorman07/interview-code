{
  "name": "@scrutinize-io/app-api",
  "private": true,
  "version": "0.0.6",
  "scripts": {
    "build": "tsc -b --incremental",
    "start": "TZ=UTC node dist/web/index.js",
    "worker": "TZ=UTC node dist/worker/index.js",
    "dev": "rm -rf dist && tsc -b --incremental && concurrently --kill-others --names 'TSC,WATCH,RUN,TEST' --prefix '({name})' --prefix-colors 'yellow.bold,yellow.bold,cyan.bold,greenBright.bold' 'tsc -b --watch --preserveWatchOutput --assumeChangesOnlyAffectDirectDependencies' 'yarn dev:web' 'yarn dev:worker' 'yarn db:migrate watch'",
    "dev:web": "nodemon --signal SIGINT --watch 'dist/nodemon.js' -x 'TZ=UTC node --inspect=9678 -r source-map-support/register -r dotenv/config' dist/web/index.js | bunyan",
    "dev:worker": "nodemon --signal SIGINT --watch 'dist/nodemon.js' -x 'TZ=UTC node --inspect=9679 -r source-map-support/register -r dotenv/config' dist/worker/index.js | bunyan",
    "test": "TEST=true TZ=UTC node ./node_modules/.bin/jest -c test/jest.config.test.js",
    "inttest": "cross-env-shell NODE_ENV=production TEST=true TZ=UTC \"ts-node ./node_modules/.bin/jest --runInBand --forceExit -c test/jest.config.inttest.js\"",
    "lint": "yarn prettier:check && eslint --ext .js,.ts,.graphql .",
    "prettier:check": "prettier --check '**/*.{js,ts,graphql,md}'",
    "prettier:format": "prettier --write --config ./.prettierrc \"**/*.{js,ts,graphql,md}\"",
    "db:migrate": "TZ=UTC node -r dotenv/config ./node_modules/.bin/graphile-migrate",
    "db:seed": "SEED=true node seeds/seed.js",
    "cli:dev": "TZ=UTC ts-node -r dotenv/config --project ./tsconfig.json src/cli/index.ts",
    "cli": "node -r dotenv/config dist/cli/index.js",
    "ci": "yarn lint && yarn build && yarn test && yarn inttest",
    "inttest:dev": "cross-env-shell TEST=true TZ=UTC \"yarn db:migrate reset --erase && yarn db:migrate watch --once && yarn inttest\"",
    "heroku-postbuild": "yarn build"
  },
